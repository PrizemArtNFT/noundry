{
    "rewrites": [
        {
            "source": "/:any*",
            "destination": "/"
        }
    ],
    "redirects": [
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "host",
                    "value": "propose.noundry.wtf"
                }
            ],
            "destination": "https://studio.noundry.wtf/propose",
            "permanent": false
        }
    ],
    "github": {
        "silent": true
    }
}